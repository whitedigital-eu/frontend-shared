  "module": "./dist/wd-frontend-shared.es.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vue-tsc --noEmit && vite build && vite preview",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "type-coverage": "type-coverage --detail",
    "safe-publish": "yarn && yarn type-check && yarn vitest run && yarn publish",
    "export-translations": "ts-node --project tsconfig.exporter.json ./i18n/exporter.ts -p '../**/*.?(ts|vue)' -s './translations.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whitedigital-eu/frontend-shared.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
