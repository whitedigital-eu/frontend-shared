  "license": "MIT",
  "description": "Shared components, helpers, composables and types for vue frontend SPAs",
  "files": [
    "site-tree",
    "config",
    "components",
    "composables",
    "helpers",
    "models",
    "types",
    "i18n",
    "default-translations.json"
